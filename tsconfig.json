{
	"compilerOptions": {
		"module": "commonjs",
		"target": "esnext",
		"skipLibCheck": true,
		"removeComments": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"sourceMap": false,
		"esModuleInterop": true,
		"noUnusedLocals": false,
		"allowUnreachableCode": false,
		"strict": true,
		"noImplicitReturns": true
	},
	"exclude": [
		"node_modules",
		"src",
		"dist"
	]
}
