{
	"compilerOptions": {
		"module": "commonjs",
		"target": "esnext",
		"removeComments": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"sourceMap": false,
		"esModuleInterop": true,
		"noUnusedLocals": false
	},
	"exclude": [
		"node_modules",
		"src",
		"dist"
	]
}
